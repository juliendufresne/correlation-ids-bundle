{
    "name": "manomano-tech/correlation-ids-bundle",
    "description": "Symfony integration of manomano-tech/correlation-ids",
    "keywords": [
        "correlation",
        "correlation-id",
        "tracing",
        "symfony",
        "symfony-bundle"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.1.3",
        "manomano-tech/correlation-ids": "^1.0",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/event-dispatcher": "^4.1",
        "symfony/http-kernel": "^4.1"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "manomano-tech/correlation-ids-guzzle": "^1.0",
        "manomano-tech/correlation-ids-httplug": "^1.0",
        "manomano-tech/correlation-ids-monolog": "^1.0",
        "php-http/client-common": "^1.7",
        "php-http/mock-client": "^1.1.0",
        "php-mock/php-mock-phpunit": "^2.1",
        "phpunit/phpunit": "^7.3",
        "ramsey/uuid": "^3.7",
        "symfony/debug": "^4.1",
        "symfony/monolog-bundle": "^3.3",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ManoManoTech\\CorrelationIdBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ManoManoTech\\CorrelationIdBundle\\Tests\\": "tests/"
        }
    }
}
